{
  "name": "@project/subgraph",
  "version": "1.0.0",
  "dependencies": {
    "@graphprotocol/graph-cli": "0.18.0",
    "@graphprotocol/graph-ts": "0.18.1"
  },
  "license": "MIT",
  "scripts": {
    "auth": "graph auth https://api.thegraph.com/ $GRAPH_ACCESS_TOKEN",
    "prepare:rinkeby": "mustache config/rinkeby.json subgraph.template.yaml > subgraph.yaml",
    "prepare:mainnet": "mustache config/mainnet.json subgraph.template.yaml > subgraph.yaml",
    "build": "graph build",
    "codegen": "graph codegen --output-dir src/types/",
    "deploy:rinkeby": "graph deploy tart-tokyo/generative-art-rinkeby --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy:mainnet": "graph deploy tart-tokyo/okazz-generative-art --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/"
  },
  "devDependencies": {
    "mustache": "^4.2.0"
  }
}
